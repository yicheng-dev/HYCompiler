FUNCTION fact :
PARAM v1
IF v1 != #1 GOTO label1
RETURN v1
GOTO label2
LABEL label1 :
t3 := v1 - #1
ARG t3
t2 := CALL fact
t1 := v1 * t2
RETURN t1
LABEL label2 :
FUNCTION main :
READ t4
v2 := t4
IF v2 <= #1 GOTO label3
ARG v2
v3 := CALL fact
GOTO label4
LABEL label3 :
v3 := #1
LABEL label4 :
WRITE v3
RETURN #0
